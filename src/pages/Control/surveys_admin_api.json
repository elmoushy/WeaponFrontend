{
  "api_info": {
    "title": "WeaponPowerCloud Surveys Admin API",
    "version": "1.0.0",
    "description": "Admin REST API for survey management, analytics, and response monitoring",
    "base_url": "/api/surveys/",
    "authentication": {
      "type": "Bearer Token (JWT)",
      "provider": "Azure Active Directory",
      "header": "Authorization: Bearer <jwt_token>",
      "description": "All endpoints require a valid Azure AD JWT token with admin or manager role"
    },
    "required_roles": ["admin", "manager"],
    "contact": {
      "name": "WeaponPowerCloud API Support",
      "email": "support@weaponpowercloud.com"
    }
  },
  "endpoints": [
    {
      "name": "List All Surveys (Admin)",
      "path": "/api/surveys/surveys/",
      "method": "GET",
      "description": "Retrieve all surveys in the system with full administrative details",
      "authentication_required": true,
      "required_roles": ["admin", "manager"],
      "parameters": {
        "query": {
          "visibility": {
            "type": "string",
            "enum": ["PRIVATE", "AUTH", "PUBLIC"],
            "description": "Filter by survey visibility",
            "example": "PRIVATE"
          },
          "is_active": {
            "type": "boolean",
            "description": "Filter by active status",
            "example": true
          },
          "creator": {
            "type": "integer",
            "description": "Filter by creator user ID",
            "example": 1
          },
          "search": {
            "type": "string",
            "description": "Search in title and description",
            "example": "employee satisfaction"
          },
          "ordering": {
            "type": "string",
            "description": "Order results by field",
            "example": "-created_at"
          }
        }
      },
      "responses": {
        "200": {
          "description": "Surveys retrieved successfully",
          "content": {
            "application/json": {
              "example": {
                "status": "success",
                "message": "Surveys retrieved successfully",
                "data": [
                  {
                    "id": "123e4567-e89b-12d3-a456-426614174000",
                    "title": "Employee Satisfaction Survey 2025",
                    "description": "Annual employee satisfaction and feedback survey",
                    "visibility": "AUTH",
                    "creator": 1,
                    "creator_email": "admin@weaponpowercloud.com",
                    "is_locked": false,
                    "is_active": true,
                    "questions": [
                      {
                        "id": "456e7890-e89b-12d3-a456-426614174001",
                        "text": "How satisfied are you with your current role?",
                        "question_type": "single_choice",
                        "options": "[\"Very satisfied\", \"Satisfied\", \"Neutral\", \"Dissatisfied\", \"Very dissatisfied\"]",
                        "is_required": true,
                        "order": 1
                      }
                    ],
                    "response_count": 45,
                    "shared_with_emails": [],
                    "created_at": "2025-08-01T10:00:00Z",
                    "updated_at": "2025-08-02T14:30:00Z"
                  }
                ]
              }
            }
          }
        }
      }
    },
    {
      "name": "Create Survey",
      "path": "/api/surveys/surveys/",
      "method": "POST",
      "description": "Create a new survey with questions",
      "authentication_required": true,
      "required_roles": ["admin", "manager"],
      "parameters": {
        "body": {
          "type": "object",
          "required": ["title"],
          "properties": {
            "title": {
              "type": "string",
              "description": "Survey title",
              "example": "Q3 Performance Review Survey"
            },
            "description": {
              "type": "string",
              "description": "Survey description",
              "example": "Quarterly performance review and goal setting survey"
            },
            "visibility": {
              "type": "string",
              "enum": ["PRIVATE", "AUTH", "PUBLIC"],
              "default": "PRIVATE",
              "description": "Survey visibility level"
            },
            "is_active": {
              "type": "boolean",
              "default": true,
              "description": "Whether survey accepts responses"
            }
          }
        }
      },
      "responses": {
        "201": {
          "description": "Survey created successfully",
          "content": {
            "application/json": {
              "example": {
                "status": "success",
                "message": "Survey created successfully",
                "data": {
                  "id": "789e0123-e89b-12d3-a456-426614174002",
                  "title": "Q3 Performance Review Survey",
                  "description": "Quarterly performance review and goal setting survey",
                  "visibility": "PRIVATE",
                  "creator": 1,
                  "creator_email": "admin@weaponpowercloud.com",
                  "is_locked": false,
                  "is_active": true,
                  "questions": [],
                  "response_count": 0,
                  "shared_with_emails": [],
                  "created_at": "2025-08-02T15:00:00Z",
                  "updated_at": "2025-08-02T15:00:00Z"
                }
              }
            }
          }
        }
      }
    },
