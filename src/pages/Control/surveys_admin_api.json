    {
      "name": "Submit Survey Response",
      "path": "/api/surveys/surveys/{survey_id}/submit/",
      "method": "POST",
      "description": "Submit responses to a survey (admin can submit on behalf of others)",
      "authentication_required": true,
      "required_roles": ["admin", "manager"],
      "parameters": {
        "path": {
          "survey_id": {
            "type": "string",
            "format": "uuid",
            "description": "Survey UUID"
          }
        },
        "body": {
          "type": "object",
          "required": ["answers"],
          "properties": {
            "answers": {
              "type": "array",
              "description": "Array of answers to survey questions",
              "items": {
                "type": "object",
                "required": ["question_id", "answer_text"],
                "properties": {
                  "question_id": {
                    "type": "string",
                    "format": "uuid",
                    "description": "Question UUID"
                  },
                  "answer_text": {
                    "type": "string",
                    "description": "Answer content"
                  }
                }
              }
            }
          }
        }
      },
      "responses": {
        "201": {
          "description": "Response submitted successfully",
          "content": {
            "application/json": {
              "example": {
                "status": "success",
                "message": "Response submitted successfully",
                "data": {
                  "response_id": "abc12345-e89b-12d3-a456-426614174003",
                  "submitted_at": "2025-08-02T14:30:00Z",
                  "answer_count": 2
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "Add Question to Survey",
      "path": "/api/surveys/surveys/{survey_id}/questions/",
      "method": "POST",
      "description": "Add a new question to an existing survey",
      "authentication_required": true,
      "required_roles": ["admin", "manager"],
      "parameters": {
        "path": {
          "survey_id": {
            "type": "string",
            "format": "uuid",
            "description": "Survey UUID",
            "example": "123e4567-e89b-12d3-a456-426614174000"
          }
        },
        "body": {
          "type": "object",
          "required": ["text", "question_type"],
          "properties": {
            "text": {
              "type": "string",
              "description": "Question text",
              "example": "How would you rate your overall job satisfaction?"
            },
            "question_type": {
              "type": "string",
              "enum": ["text", "textarea", "single_choice", "multiple_choice", "rating", "yes_no"],
              "description": "Type of question",
              "example": "single_choice"
            },
            "options": {
              "type": "string",
              "description": "JSON array of options for choice questions",
              "example": "[\"Excellent\", \"Good\", \"Fair\", \"Poor\"]"
            },
            "is_required": {
              "type": "boolean",
              "default": true,
              "description": "Whether the question is required"
            },
            "order": {
              "type": "integer",
              "description": "Question order (auto-incremented if not provided)"
            }
          }
        }
      },
      "responses": {
        "201": {
          "description": "Question added successfully",
          "content": {
            "application/json": {
              "example": {
                "status": "success",
                "message": "Question added successfully",
                "data": {
                  "id": "456e7890-e89b-12d3-a456-426614174001",
                  "text": "How would you rate your overall job satisfaction?",
                  "question_type": "single_choice",
                  "options": "[\"Excellent\", \"Good\", \"Fair\", \"Poor\"]",
                  "is_required": true,
                  "order": 1,
                  "survey": "123e4567-e89b-12d3-a456-426614174000",
                  "created_at": "2025-08-03T14:30:00Z",
                  "updated_at": "2025-08-03T14:30:00Z"
                }
              }
            }
          }
        },
        "400": {
          "description": "Invalid question data",
          "content": {
            "application/json": {
              "example": {
                "status": "error",
                "message": "Invalid question data",
                "data": {
                  "text": ["This field is required."]
                }
              }
            }
          }
        },
        "409": {
          "description": "Cannot add questions to locked survey",
          "content": {
            "application/json": {
              "example": {
                "status": "error",
                "message": "Cannot add questions to locked survey"
              }
            }
          }
        }
      }
    },
