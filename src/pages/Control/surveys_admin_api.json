    {
      "name": "Bulk Survey Operations",
      "path": "/api/surveys/bulk-operations/",
      "method": "POST",
      "description": "Perform bulk operations on multiple surveys (admin only)",
      "authentication_required": true,
      "required_roles": ["admin"],
      "parameters": {
        "body": {
          "type": "object",
          "required": ["operation", "survey_ids"],
          "properties": {
            "operation": {
              "type": "string",
              "enum": ["activate", "deactivate", "lock", "unlock", "delete"],
              "description": "Bulk operation to perform",
              "example": "activate"
            },
            "survey_ids": {
              "type": "array",
              "items": {
                "type": "string",
                "format": "uuid"
              },
              "description": "Array of survey UUIDs to operate on",
              "example": [
                "123e4567-e89b-12d3-a456-426614174000",
                "789e0123-e89b-12d3-a456-426614174002"
              ]
            }
          }
        }
      },
      "responses": {
        "200": {
          "description": "Bulk operation completed",
          "content": {
            "application/json": {
              "example": {
                "status": "success",
                "message": "Bulk operation completed",
                "data": {
                  "operation": "activate",
                  "successful": 2,
                  "failed": 0,
                  "errors": []
                }
              }
            }
          }
        },
        "400": {
          "description": "Invalid operation or missing survey IDs"
        },
        "403": {
          "description": "Only administrators can perform bulk operations"
        }
      }
    },
