    {
      "name": "Update Survey",
      "path": "/api/surveys/surveys/{survey_id}/",
      "method": "PATCH",
      "description": "Update survey details (only by creator unless admin)",
      "authentication_required": true,
      "required_roles": ["admin", "manager"],
      "parameters": {
        "path": {
          "survey_id": {
            "type": "string",
            "format": "uuid",
            "description": "Survey UUID",
            "example": "123e4567-e89b-12d3-a456-426614174000"
          }
        },
        "body": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string",
              "description": "Survey title"
            },
            "description": {
              "type": "string",
              "description": "Survey description"
            },
            "is_active": {
              "type": "boolean",
              "description": "Whether survey accepts responses"
            },
            "is_locked": {
              "type": "boolean",
              "description": "Lock survey to prevent further modifications"
            }
          }
        }
      },
      "responses": {
        "200": {
          "description": "Survey updated successfully"
        },
        "403": {
          "description": "Access denied - only survey creator or admin can modify"
        },
        "409": {
          "description": "Cannot modify locked survey"
        }
      }
    },
    {
      "name": "Delete Survey",
      "path": "/api/surveys/surveys/{survey_id}/",
      "method": "DELETE",
      "description": "Soft delete a survey (only by creator unless admin)",
      "authentication_required": true,
      "required_roles": ["admin", "manager"],
      "parameters": {
        "path": {
          "survey_id": {
            "type": "string",
            "format": "uuid",
            "description": "Survey UUID"
          }
        }
      },
      "responses": {
        "204": {
          "description": "Survey deleted successfully"
        },
        "403": {
          "description": "Access denied - only survey creator or admin can delete"
        }
      }
    },
    {
      "name": "Set Survey Audience",
      "path": "/api/surveys/surveys/{survey_id}/audience/",
      "method": "POST",
      "description": "Configure survey visibility and sharing settings",
      "authentication_required": true,
      "required_roles": ["admin", "manager"],
      "parameters": {
        "path": {
          "survey_id": {
            "type": "string",
            "format": "uuid",
            "description": "Survey UUID"
          }
        },
        "body": {
          "type": "object",
          "required": ["visibility"],
          "properties": {
            "visibility": {
              "type": "string",
              "enum": ["PRIVATE", "AUTH", "PUBLIC"],
              "description": "Survey visibility level"
            },
            "user_ids": {
              "type": "array",
              "items": {
                "type": "integer"
              },
              "description": "User IDs to share with (only for PRIVATE surveys)",
              "example": [5, 10, 15]
            }
          }
        }
      },
      "responses": {
        "200": {
          "description": "Survey audience updated successfully",
          "content": {
            "application/json": {
              "example": {
                "status": "success",
                "message": "Survey audience updated successfully",
                "data": {
                  "visibility": "PRIVATE",
                  "shared_count": 3
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "Clone Survey",
